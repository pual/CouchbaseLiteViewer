//  Build settings for true (dylib-based) frameworks

DEBUG_INFORMATION_FORMAT     = dwarf-with-dsym  // dSYM file allows debugging inside framework
DEFINES_MODULE               = YES
DYLIB_COMPATIBILITY_VERSION  = 1
DYLIB_CURRENT_VERSION        = 1
FRAMEWORK_VERSION            = A
WRAPPER_EXTENSION            = framework
